{
  "name": "template-node-stack",
  "version": "0.0.1",
  "description": "Template for Koa + TS + Sequelize microservice",
  "main": "server/index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "BABEL_DISABLE_CACHE=1 ./node_modules/.bin/mocha test/test.bootstap.js src/**/**.specs.js | ./node_modules/.bin/pino",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run build:types && npm run build:js && npm run build:public",
    "build:public": "webpack --mode production",
    "build:js": "npm run clean && npm i && BABEL_DISABLE_CACHE=1 babel src --copy-files --source-maps=inline --out-dir dist --extensions \".js,.ts\"",
    "start": "node ./dist/server/index",
    "migrate-start": "npm run start",
    "watch-public": "webpack-dev-server --config ./webpack.config.js --mode development",
    "watch": "nodemon ./src/server/index | ./node_modules/.bin/pino",
    "watch-test": "nodemon ./src/server/index --exec 'npm run test'",
    "coverage": "BABEL_DISABLE_CACHE=1 NODE_ENV=test ./node_modules/.bin/nyc npm test",
    "clean": "rm -rf dist && rm -rf .nyc_output && rm -rf report-dir"
  },
  "dependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/node": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-typescript": "7.1.0",
    "@babel/runtime": "7.1.2",
    "apollo-server-koa": "1.4.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-inline-import": "2.0.6",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-object-rest-spread": "7.0.0-beta.3",
    "graphql": "0.13.2",
    "graphql-tools": "3.1.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "koa": "2.5.2",
    "koa-bodyparser": "4.2.0",
    "koa-compose": "4.1.0",
    "koa-jwt": "3.5.1",
    "koa-router": "7.4.0",
    "koa-static": "^5.0.0",
    "node-fetch": "2.2.0",
    "pino": "4.17.6",
    "sequelize": "4.42.0",
    "sequelize-cli": "5.4.0",
    "spdy": "3.4.7",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2"
  },
  "devDependencies": {
    "@types/graphql": "0.13.2",
    "@types/koa-router": "7.0.32",
    "@types/pino": "5.8.3",
    "babel-plugin-istanbul": "4.1.6",
    "chai": "4.1.2",
    "chai-http": "4.2.0",
    "cross-env": "5.1.6",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.10.0",
    "eslint-import-resolver-babel-module": "5.0.0-beta.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-typescript": "0.14.0",
    "istanbul": "0.4.5",
    "koa-webpack": "^5.1.1",
    "mocha": "5.1.1",
    "nock": "9.6.1",
    "nodemon": "1.18.4",
    "nyc": "11.8.0",
    "prettier": "1.12.1",
    "prettier-tslint": "^0.4.1",
    "ts-node": "7.0.1",
    "tslint": "^5.12.0",
    "typescript": "3.2.2",
    "typescript-eslint-parser": "21.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.axa.com/it-digital/myaxa-conversation.git"
  },
  "author": "Emanuele Di Saverio <emanuele.disaverio@gmail.com>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.0.0"
  }
}
