{
  "name": "template-node-stack",
  "version": "0.0.1",
  "description": "Template for Koa + TS + Sequelize WebApp | Micri",
  "main": "src/server/index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "BABEL_DISABLE_CACHE=1 ./node_modules/.bin/mocha | ./node_modules/.bin/pino",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run build:types && npm run build:js && npm run build:public",
    "build:public": "webpack --mode production",
    "build:js": "BABEL_DISABLE_CACHE=1 babel src --copy-files --source-maps=inline --out-dir dist --extensions \".js,.ts\"",
    "start": "node ./dist/server/index",
    "migrate-start": "npm run start",
    "watch": "rm -rf dist && (nodemon ./src/server/index | ./node_modules/.bin/pino)",
    "watch2": "nodemon ./src/server/test.js",
    "watch-test": "nodemon ./src/server/index --exec 'npm run test'",
    "coverage": "BABEL_DISABLE_CACHE=1 NODE_ENV=test ./node_modules/.bin/nyc npm test",
    "clean": "rm -rf dist && rm -rf .nyc_output && rm -rf report-dir",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/node": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-typescript": "7.1.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "7.1.2",
    "@types/mocha": "^5.2.5",
    "apollo-server-koa": "1.4.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-file-loader": "^1.1.1",
    "babel-plugin-inline-import": "2.0.6",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-object-rest-spread": "7.0.0-beta.3",
    "css-file-loader": "^1.4.3",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "graphql": "0.13.2",
    "graphql-tools": "3.1.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jsonwebtoken": "^8.4.0",
    "koa": "2.5.2",
    "koa-bodyparser": "4.2.0",
    "koa-compose": "4.1.0",
    "koa-jwt": "3.5.1",
    "koa-router": "7.4.0",
    "koa-static": "^5.0.0",
    "node-fetch": "2.2.0",
    "pino": "4.17.6",
    "sequelize": "4.42.0",
    "sequelize-cli": "5.4.0",
    "spdy": "3.4.7",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2",
    "typescript": "3.2.2"
  },
  "devDependencies": {
    "@types/graphql": "0.13.2",
    "@types/koa-bodyparser": "^4.2.1",
    "@types/koa-router": "7.0.32",
    "@types/koa-static": "^4.0.0",
    "@types/koa-webpack": "^5.0.1",
    "@types/lodash": "^4.14.119",
    "@types/node-fetch": "^2.1.4",
    "@types/pino": "5.8.3",
    "@types/spdy": "^3.4.4",
    "@types/webpack": "^4.4.22",
    "babel-plugin-istanbul": "4.1.6",
    "chai": "4.1.2",
    "chai-http": "4.2.0",
    "cross-env": "5.1.6",
    "eslint": "4.19.1",
    "eslint-import-resolver-babel-module": "5.0.0-beta.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-typescript": "0.14.0",
    "istanbul": "0.4.5",
    "koa-webpack": "^5.1.1",
    "mocha": "5.1.1",
    "nock": "9.6.1",
    "nodemon": "1.18.9",
    "nyc": "11.8.0",
    "ts-node": "7.0.1",
    "tslint": "^5.12.0",
    "typescript-eslint-parser": "21.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frog/template-node-stack.git"
  },
  "author": "Emanuele Di Saverio <emanuele.disaverio@gmail.com>",
  "license": "Apache-2.0",
  "engines": {
    "node": "8.11.1"
  }
}